{
  "name": "@kainstar/svelte-i18next",
  "version": "1.0.1",
  "description": "i18next's framework adapter for svelte, better internationalization UX",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "svelte": "./dist/index.js"
    }
  },
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "svelte-package -i src",
    "dev": "cd example && vite",
    "cm": "cz",
    "clean": "rimraf lib",
    "lint": "eslint src",
    "prepare": "is-ci || husky",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kainstar/svelte-i18next.git"
  },
  "license": "MIT",
  "author": {
    "name": "Kainstar",
    "email": "15249633+kainstar@users.noreply.github.com",
    "url": "https://github.com/kainstar"
  },
  "engines": {
    "node": ">=18.0"
  },
  "keywords": [
    "svelte",
    "i18n",
    "i18next",
    "internationalization"
  ],
  "bugs": {
    "url": "https://github.com/kainstar/svelte-i18next/issues"
  },
  "homepage": "https://github.com/kainstar/svelte-i18next#readme",
  "devDependencies": {
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.1.0",
    "@kainstar/eslint-config": "^1.1.1",
    "@kainstar/vite-plugin-i18next-loader": "^1.0.3",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^12.0.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^10.0.2",
    "@semantic-release/npm": "^12.0.0",
    "@semantic-release/release-notes-generator": "^12.1.0",
    "@sveltejs/package": "^2.3.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.2",
    "@tsconfig/svelte": "^5.0.2",
    "@types/eslint": "^8.56.5",
    "@types/node": "^20.11.29",
    "@typescript-eslint/parser": "^7.3.1",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "npm:eslint-ts-patch@8.56.0-0",
    "eslint-plugin-svelte": "^2.35.1",
    "eslint-ts-patch": "8.57.0-0",
    "husky": "^9.0.11",
    "i18next": "^23.10.1",
    "is-ci": "^3.0.1",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "prettier-plugin-svelte": "^3.2.2",
    "rimraf": "^5.0.5",
    "sass": "^1.72.0",
    "semantic-release": "^23.0.5",
    "svelte": "^4.2.12",
    "svelte-eslint-parser": "^0.33.1",
    "typescript": "^5.4.2",
    "vite": "^5.1.6"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "i18next": "^23.10.1",
    "svelte": "^4.2.12"
  },
  "dependencies": {
    "html-parse-stringify": "^3.0.1"
  }
}
